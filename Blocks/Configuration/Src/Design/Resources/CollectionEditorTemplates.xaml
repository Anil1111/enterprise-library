    <DataTemplate x:Key="OraclePackageDataViewModelItem">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition SharedSizeGroup="PrefixCol" MinWidth="60" Width="*" />
                <ColumnDefinition SharedSizeGroup="NameCol" MinWidth="60" Width="*" />
                <ColumnDefinition SharedSizeGroup="DeleteCol" Width="16"/>
            </Grid.ColumnDefinitions>

            <ContentControl Grid.Column="0" >
                <ContentPresenter Content="{Binding PrefixProperty}"  ContentTemplate="{DynamicResource TextPropertyEditingTemplate}" />
            </ContentControl>
            
            <ContentControl Grid.Column="1" >
                <ContentPresenter Content="{Binding NameProperty}"  ContentTemplate="{DynamicResource TextPropertyEditingTemplate}" />
            </ContentControl>

            <Button Grid.Column="2" x:Name="DeleteButton"  Command="{Binding DeleteCommand}" Style="{DynamicResource DeleteButtonStyle}" Margin="0,0,0,8" VerticalAlignment="Top" Width="7" Height="7" />
        </Grid>
    </DataTemplate>

